<turbo-frame id="feedback_content">
<div data-toggle-target="feedback" data-success="<%= @question.successful_title && @question.successful_artist %>">
  <% if @question.successful_title && @question.successful_artist %>
  <div class="d-block">
    <h2>Correct!</h2>
    <div> Title: <%= question.song.title %>, you said: <%= question.user_answer_title %>. Correct? <%= question.successful_title.to_s %>.</div>
    <div> Artist: <%= question.song.artist %>, you said: <%= question.user_answer_artist %>. Correct? <%= question.successful_artist.to_s %>.</div>
  </div>
  <% else %>
  <h2>Incorrect</h2>
  <% end %>
</div>
</turbo-frame>
